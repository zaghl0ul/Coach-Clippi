{
  "name": "slippi-coach",
  "version": "1.0.0",
  "description": "A project that interprets and reads Slippi files, providing live coaching and commentary using AI models.",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "start:file": "node src/alt-index.js",
    "start:realtime": "node src/realTimeIndex.js",
    "coach": "node src/enhancedcoach.js",
    "offline-coach": "node src/enhancedcoach.js",
    "test": "echo \"No tests specified\" && exit 0",
    "dev": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "start:backend": "node src/index.js",
    "start:webserver": "node src/webServer.js",
    "start:frontend": "cd frontend && npm run dev",
    "build:frontend": "cd frontend && npm run build",
    "install:frontend": "cd frontend && npm install"
  },
  "dependencies": {
    "@slippi/slippi-js": "^6.0.0",
    "@vinceau/slp-realtime": "^3.0.0",
    "axios": "^0.21.1",
    "chokidar": "^3.5.3",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.18.2",
    "fs": "0.0.1-security",
    "lodash": "^4.17.21",
    "multer": "^1.4.5-lts.1",
    "nan": "^2.23.0",
    "readline": "^1.3.0",
    "socket.io": "^4.7.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "node-gyp": "^11.4.2",
    "nodemon": "^2.0.7"
  },
  "keywords": [
    "slippi",
    "coaching",
    "commentary",
    "smash-bros"
  ],
  "author": "Your Name",
  "license": "MIT"
}
